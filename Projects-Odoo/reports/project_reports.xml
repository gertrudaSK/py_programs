<odoo>
    <report
            id="report_project"
            model="projektai.project"
            string="Project Report"
            name="projektai.report_project_view"
            file="projektai.report_project"
            report_type="qweb-pdf"/>

    <template id="report_project_view">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2 t-field="doc.name"/>
                        <p>From
                            <span t-field="doc.start_date"/>
                            to
                            <span
                                    t-field="doc.end_date"/>
                        </p>
                        <h3>Employees:</h3>
                        <ul>
                            <t t-foreach="doc.darbuotojas_ids"
                               t-as="darbuotojas">
                                <li>
                                    <span t-field="darbuotojas.name"/>
                                </li>
                            </t>
                        </ul>
                    </div>
                </t>
            </t>
        </t>
    </template>

</odoo>